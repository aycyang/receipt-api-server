<h1>Receipt Printer API</h1>
<h2>Recurse Center</h2>
<% if (name) { %>
<p>Hello, <%= name %>! <a href="/logout">Log out</a></p>
<% } else { %>
<p>Welcome! <a href="/login">Log in</a></p>
<% } %>
<script>
const intendedOrigin = '<%= origin %>'
const actualOrigin = window.location.origin
if (intendedOrigin !== actualOrigin) {
  window.location.href = intendedOrigin
}
</script>
